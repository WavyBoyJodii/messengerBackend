{
  "name": "messengerbackend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rimraf dist && tsc",
    "prestart": "npm run build",
    "start": "node ./bin/www",
    "predevStart": "npm run build",
    "devStart": "concurrently \"tsc -w\" \" nodemon ./bin/www\"",
    "generate": "drizzle-kit generate:pg",
    "migrate": "node dist/db/migrate.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.1",
    "express": "~4.16.1",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
